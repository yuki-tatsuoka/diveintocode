    "# 変数定義\n",
    "rice_count = 1\n",
    "rice_count_list = [rice_count]\n",
    "total_rice = 1\n",
    "total_rice_list = [rice_count]\n",
    "days = 100\n",
    "\n",
    "# 日にち毎にもらえる米の量と、累積米の量\n",
    "for day in range(1, days + 1): #range(days)にすると０日目の米の量が表示されてしまうため\n",
    "    rice_count = rice_count_list[-1]  * 2    \n",
    "    rice_count_list.append(rice_count)\n",
    "    total_rice += rice_count_list[-1]\n",
    "    total_rice_list.append(total_rice)\n",
    "\n",
    "#正しいか確認\n",
    "print(rice_count_list)   \n",
    "print(total_rice_list)\n",
    "\n",
    "\n",
    "#図形化\n",
    "import matplotlib.pyplot as plt\n",
    "import warnings\n",
    "warnings.simplefilter(\"ignore\")\n",
    "\n",
    "plt.title(\"rice_count\")\n",
    "plt.xlabel(\"days\")\n",
    "plt.ylabel(\"rice_count_morment\")\n",
